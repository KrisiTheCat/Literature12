<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!-- regular -->
    <font
        android:font="@font/source_extralight"
        android:fontStyle="normal"
        android:fontWeight="100"
        app:font="@font/source_extralight"
        app:fontStyle="normal"
        app:fontWeight="100"/>
    <font
        android:font="@font/source_light"
        android:fontStyle="normal"
        android:fontWeight="200"
        app:font="@font/source_light"
        app:fontStyle="normal"
        app:fontWeight="200"/>
    <font
        android:font="@font/source_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/source_regular"
        app:fontStyle="normal"
        app:fontWeight="400"/>
    <font
        android:font="@font/source_semibold"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:font="@font/source_semibold"
        app:fontStyle="normal"
        app:fontWeight="500"/>
    <font
        android:font="@font/source_bold"
        android:fontStyle="normal"
        android:fontWeight="600"
        app:font="@font/source_bold"
        app:fontStyle="normal"
        app:fontWeight="600"/>
    <font
        android:font="@font/source_black"
        android:fontStyle="normal"
        android:fontWeight="800"
        app:font="@font/source_black"
        app:fontStyle="normal"
        app:fontWeight="800"/>

</font-family>